</var_value>
  <make></make>
<param>
	  <name>Constellation Type</name>
	  <key>type</key>
	  <type>enum</type>
	  <option>
		  <name>BPSK</name>
		  <key>bpsk</key>
	  </option>
	  <option>
		  <name>QPSK</name>
		  <key>qpsk</key>
	  </option>
  </param>
</block>
